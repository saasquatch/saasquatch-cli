{
  "name": "saasquatch-cli",
  "description": "Command Line (CLI) for Referral SaaSquatch",
  "version": "1.4.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/saasquatch/saasquatch-cli.git"
  },
  "bin": {
    "squatch": "./dist/index.js"
  },
  "scripts": {
