      "babel-plugin-transform-es2015-modules-commonjs"
    ]
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.0",
    "apollo-link-http": "^1.3.2",
    "babel-polyfill": "^6.26.0",
    "base-64": "^0.1.0",
    "chalk": "^1.1.1",
    "commander": "^2.16.0",
    "figures": "^2.0.0",
    "glob": "^7.1.2",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.6.1",
    "ink": "^0.5.0",
    "ink-password-input": "^2.0.0",
    "ink-spinner": "^2.0.0",
    "ink-text-input": "^2.0.0",
    "locale-code": "^2.0.1",
